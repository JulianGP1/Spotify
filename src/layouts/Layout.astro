				<input
					class="bg-gray-900 placeholder:text-gray-300"
					type="text"
					name=""
					id=""
					placeholder="¿Qué quieres reproducir?"
				/>
			</div>

			<div class="relative group">
				<a href="#" class="mr-7 font-bold text-[18px] text-gray-300">Registrarse</a>
				<div class="absolute left-0 mt-[20px] w-40 bg-black opacity-0 group-hover:opacity-100 transition-opacity duration-200">
					<a href="#" class="block text-white text-lg py-2 px-4 hover:bg-gray-700">Crear usuario</a>
				</div>
			</div>

			<div
				class="mr-6 h-[45px] text-[18px] bg-white rounded-3xl flex items-center justify-center w-[170px]"
			>
				<a href="" class="text-black font-bold">Iniciar Sesión</a>
			</div>
		</div>
	
